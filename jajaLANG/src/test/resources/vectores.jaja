incognito dato_t = registro {
    atr1: ent @,
    atr2: vector(ent @, 2),
};

vector(ent @, 2) v1;
vector(dato_t, 2) v2;

diver tronco(){

    v1 = nuevo [nuevo ent, nuevo ent];

    ent @ a;
    ent @ b;

    (@a) = 5;
    (@b) = 10;

    v1[0]=a;
    v1[0]=b;


    dato_t reg = nuevo {atr1 = a, atr2 = v1 };

    escribirEnt(@reg.atr1); //tiene que ser 5
    reg.atr1+=1;
    escribirEnt(@reg.atr2[1]); // tiene que ser 6;


    liberar(a);
    liberar(b);
}
