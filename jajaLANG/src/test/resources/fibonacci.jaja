#traficar src/test/resources/parImpar.jaja como r

diver fibonacci(c: ent) -> ent {
    si c == 0 {
        devuelve 1; 
    }
    si c == 1 {
        devuelve 1;
    }
    devuelve fibonacci(c-1) + fibonacci(c-2);
}

diver tronco() {
    //Imprime algo por pantalla
    escribirBin(r::par(2));

    para i = 0 -> 10 {
        escribirEntero(fibonacci(i));
    } 
}
